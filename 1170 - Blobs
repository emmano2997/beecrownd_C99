#include <stdio.h>

int main()
{
    int teste, dia = 0;
    float comida;
    scanf("%d",&teste);
    
    for(int i = 0; teste > i; i++){
        scanf("%f",&comida);
        dia = 0;
        while (comida>1){
            comida = comida /2;
            dia ++;
            
        }
        printf("%d dias\n",dia);
    }
    return 0;
}
