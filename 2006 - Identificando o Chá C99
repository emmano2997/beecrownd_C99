#include <stdio.h>

int main()
{
    int input, cha ,cont=0;
    scanf("%d", &input);
    for(int i=0; i<5; i++)
    {
        scanf("%d", &cha);
        if(cha==input) cont++;
    }
    printf("%d\n", cont);
    return 0;
}

