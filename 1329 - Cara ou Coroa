*******************************************************************************/
#include <stdio.h>

int main(void){
    int n, r, maria, joao, i;

    n = -1;
    while(n != 0){
        scanf("%d", &n);
        maria = joao = 0;
        for(i = 0; i < n; i++){
            scanf("%d", &r);
            if (r){
                joao++;
            }else{
                maria++;
            }
        }
        if (n != 0)
            printf("Mary won %d times and John won %d times\n", maria, joao);
    }


    return 0;
}


